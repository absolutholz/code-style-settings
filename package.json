{
  "name": "penny.de-relaunch",
  "version": "1.0.0",
  "description": "EditorConfig , ESLint, stylelint, Prettier and Visual Studio Code",
  "main": "package.json",
  "scripts": {
    "scripts:lint": "eslint --fix \"**/*.js\"",
    "scripts:prettier": "prettier --write \"**/*.js\"",
    "scripts": "npm run scripts:prettier && npm run scripts:lint",
    "styles:lint": "stylelint --fix \"**/*.{css,scss}\"",
    "styles:prettier": "prettier --write \"**/*.{css,scss}\"",
    "styles": "npm run styles:lint && npm run styles:prettier"
  },
  "keywords": [
    "editorconfig",
    "prettier",
    "eslint",
    "stylelint",
    "vscode"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/absolutholz/code-style-settings.git"
  },
  "author": "absolutholz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/absolutholz/code-style-settings/issues"
  },
  "homepage": "https://github.com/absolutholz/code-style-settings#readme",
  "dependencies": {
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.18.2",
    "stylelint": "^11.0.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.11.0"
  }
}
